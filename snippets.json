{
  "html": {
    "snippets": {
      "!": "{<!DOCTYPE html>}>html[lang='${lang}']>(head>meta[charset='${charset}']+meta[http-equiv='X-UA-Compatible'][content='IE=edge']+meta[name='viewport'][content='width=device-width,initial-scale=1.0']+link:css+title>{${title}${1}})+body",
      "header": "header[role='banner']",
      "main": "main[id='main'][role='main'][tabindex='-1']",
      "footer": "footer[role='contentinfo']",
      "nav": "nav[role='navigation']",
      "nav:skip": "nav[role='navigation'][aria-label='Accès rapide']>a.skiplink[href='#main']>{Contenu principal}",
      "form" : "form[role='form'][action]",
      "form:get" : "form[role='form'][action][method='get']",
      "form:post" : "form[role='form'][action][method='post']",
      "table:mef": "table[role='presentation']",
      "table:simple" : "table>caption+(tr>th[scope='col'])+(tr>td)",
      "table:complexe" : "table>(caption>span.sr-only)+(tr>td[aria-hidden='true']+th[id='entete1'][colspan='2'])+(tr>td[aria-hidden='true']+th[id='entete2'])+(tr>td+td[headers='entete1 entete2'])",
      "embed": "embed[src][type][title]",
      "iframe": "iframe[src][frameborder='0'][title]",
      "img:svg": "img[src][alt][role='img']",
      "time": "time[datetime]",
      "svg": "svg",
      "svg:info": "svg[role='img'][aria-label]",
      "svg:deco": "svg[aria-hidden='true']",
      "figure": "figure[role='figure'][aria-label='texte figcaption']>img[src][alt='label-image']+figcaption>{[label-image] Légende}",
      "address:mailtel": "address>a:mail+a:tel",
      "bdi": "bdi",
      "blockquote": "blockquote>p",
      "blockquote:cite": "blockquote[cite='https://']>p",
      "cite:link": "cite>a",
      "data": "data[value]",
      "dl:def": "dl>dt+dd",
      "details:summary": "details>summary+p",
      "dialog": "dialog",
      "mark": "mark",
      "meter": "meter[min][max][value]",
      "ol:4": "ol>li*4>{Objet de liste $}",
      "optgroup": "optgroup[label]>option*3",
      "q:cite": "q[cite='https://']",
      "rp": "rp",
      "rt": "rt",
      "ruby": "ruby",
      "select:options": "select>(option[value='valeur $']>{Option $})*3",
      "track": "track[kind][srclang][src]",
      "ul:4": "ul>li*4>{Objet de liste $}",
      "ul:links": "ul>(li>a:link>{Lien $})*4",
      "wbr": "wbr"
    }
  }
}

